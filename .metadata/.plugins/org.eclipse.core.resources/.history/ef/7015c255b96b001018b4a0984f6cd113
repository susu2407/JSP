	<body>
		<h3>반복문</h3>
		
		<h4>for</h4>
		<%
			for(int i=1 ; i<=5 ; i++) {
				out.println("<p>i : " + i + "</p>");
			}
		%>
		
		<% for(int j=1 ; j<=5 ; j++) { %>
			<p>j : <%= j %></p>
		<% } %>		
		
		<h4>while</h4>
		<%
			int k = 1;
			while(k <= 10) {
		%>
			<p>k : <%= k %></p>
		<%
				k++;
			}
		%>
		
